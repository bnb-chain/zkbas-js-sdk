{
  "name": "@bnb-chain/zkbas-js-sdk",
  "version": "0.3.0",
  "description": "zkbas JavaScript SDK",
  "type": "commonjs",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "main": "./dist/node/index.js",
  "files": [
    "src",
    "dist/types/*.d.ts",
    "dist/*.js",
    "dist/*.cjs"
  ],
  "browser": {
    "./dist/zecrey/cjs/node.index.js": "./dist/zecrey/cjs/web.index.js",
    "./dist/zecrey/esm/node.index.js": "./dist/zecrey/esm/web.index.js"
  },
  "exports": {
    ".": {
      "import": {
        "node": "./dist/node/index.js",
        "default": "./dist/web/zk.js"
      },
      "require": "./dist/node/index.js"
    },
    "./dist/*": "./dist/*",
    "./types/*": "./types/*"
  },
  "scripts": {
    "dev": "rm -fr ./dist && tsc -p tsconfig.json",
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bnb-chain/zkbas-js-sdk.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bnb-chain/zkbas-js-sdk/issues"
  },
  "homepage": "https://github.com/bnb-chain/zkbas-js-sdk#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@rollup/plugin-wasm": "^5.2.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "rollup": "^2.77.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4"
  }
}
